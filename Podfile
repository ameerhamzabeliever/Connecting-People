# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Quokka' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Quokka
  # Overlay Controller
  pod 'DTOverlayController'
  # ProgressView
  pod 'LinearProgressView'
  # SnapKit
  pod 'SnapSDK'
  # Kingfisher
  pod 'Kingfisher'
  # Swifty Json
  pod 'SwiftyJSON'
  # Almofire
  pod 'Alamofire', '~> 4.7'
  # MenuBar
  pod 'XLPagerTabStrip', '~> 9.0'
  # Skelton View For Loading
  pod 'SkeletonView'
  # AgoraKit for VOIP
  pod 'AgoraRtcEngine_iOS'
# Pods for dynamicLinks
  pod 'Firebase/Analytics'
  pod 'Firebase/DynamicLinks'
# pod for device type recognition
  pod 'Device.swift'
 # Socket
  pod 'Socket.IO-Client-Swift', '~> 15.2.0'
end
